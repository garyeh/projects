<!DOCTYPE html>
<html>
  <head>
    <title>GoalApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>


  <body>
    <% if current_user %>
    <form action="<%= session_url %>" method="POST">
      <%= button_to "Log out", session_url, method: :delete %>
    <% else %>
      <input type ="submit" value="Sign In">
    <% end %>

    <%= yield %>
  </body>
</html>
